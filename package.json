{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "API Gateway for microservice Architecture built in core node js.",
  "main": "index.ts",
  "scripts": {
    "test": "mocha --recursive --require ts-node/register spec src/**/*.spec.ts",
    "watch": "tsc --watch",
    "build": "tsc",
    "start": "tsc && node build/index.js",
    "dev:tsc": "tsc --w",
    "dev:node": "nodemon build/index.js",
    "dev": "concurrently npm:dev:*",
    "prettier": "prettier --write src/*.ts src/**/*.ts src/**/**/*.ts src/**/**/**/*.ts"
  },
  "keywords": [
    "api-gateway"
  ],
  "author": "ketav chotaliya",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/express-fileupload": "^1.1.2",
    "@types/helmet": "0.0.45",
    "@types/js-yaml": "^3.12.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/uuid": "^7.0.0",
    "@types/winston": "^2.4.4",
    "@types/got": "^9.6.11",
    "@types/validator": "^10.11.1",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "fs": "0.0.1-security",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "winston": "^2.4.4",
    "got": "^9.6.0",
    "validator": "^11.1.0"
  }
}
